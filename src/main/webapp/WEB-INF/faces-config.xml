<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>*.xhtml</url-pattern>
</servlet-mapping>
<navigation-rule>
	<from-view-id>anamana.xhtml</from-view-id>
	<navigation-case>
		<from-action>#{pageController.newana}</from-action>
		<from-outcome>success</from-outcome>
		<to-view-id>newana.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
		<from-action>#{pageController.configurazione}</from-action>
		<from-outcome>success</from-outcome>
		<to-view-id>configurazione.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>	

</faces-config>


